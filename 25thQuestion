//print right hand triangle using stars .

#include <iostream>
#include <cmath>
using namespace std;
int main(){
    int n , i = 1;
    cout << "Enter a number: ";
    cin >> n;
    while(i <= n){
        int j = 1;
        while(j <= i){
            cout<<"*";
            j++;
        }
        cout<<endl;
        i++;

    }
}
