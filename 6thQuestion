//reverse an array?

#include <iostream>
#include <cmath>
using namespace std;
void newarry(int arr[] , int size)
{
    for (int i = 0 ; i<size ; i++)
    {
        cout<< arr[i] << " ";
    }
}
void swp(int arr[] , int size)
{
    int start = 0;
    int end = size-1;
    while(start<=end)
    {
        swap(arr[start] , arr[end]);
        start++;
        end--;
    }
}
int main()
{
    int arr[5] = {1,2,3,4,5};
    swp(arr , 5);
    newarry(arr , 5);
}
