//print the pattern of AAA
                       BBB
                       CCC

#include <iostream>
#include <cmath>
using namespace std;
int main(){
    int n , i = 1;
    cout << "Enter a number: ";
    cin >> n;
    while(i<=n){
        int j = 1;
    while(j<=n){
        char ch = 'A'+i-1;
        cout << ch << " ";
        // ch++;
        j++;
    }
        cout<<endl;
        i++;
    }
    return 0;

}
